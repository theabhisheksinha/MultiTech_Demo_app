<?xml version="1.0"?>
<!DOCTYPE ejb-jar PUBLIC '-//Sun Microsystems, Inc.//DTD Enterprise JavaBeans 1.1//EN' 'http://java.sun.com/j2ee/dtds/ejb-jar_1_1.dtd'>
<ejb-jar>
   <enterprise-beans>
      <entity>
         <description>
            Entity BMP EJB for CASTPUBS.AUTHOR table
         </description>
         <ejb-name>AuthorBean</ejb-name>
         <home>com.castsoftware.castpubs.AuthorBeanHome</home>
         <remote>com.castsoftware.castpubs.AuthorBean</remote>
         <ejb-class>com.castsoftware.castpubs.AuthorBeanEJB</ejb-class>
         <persistence-type>Bean</persistence-type>
         <prim-key-class>com.castsoftware.castpubs.AuthorBeanPK</prim-key-class>
         <reentrant>False</reentrant>
         <env-entry>
            <env-entry-name>UI</env-entry-name>
            <env-entry-type>java.lang.String</env-entry-type>
            <env-entry-value>null</env-entry-value>
         </env-entry>
         <env-entry>
            <env-entry-name>LName</env-entry-name>
            <env-entry-type>java.lang.String</env-entry-type>
            <env-entry-value>null</env-entry-value>
         </env-entry>
         <env-entry>
            <env-entry-name>FName</env-entry-name>
            <env-entry-type>java.lang.String</env-entry-type>
            <env-entry-value>null</env-entry-value>
         </env-entry>
      </entity>
   </enterprise-beans>

   <assembly-descriptor>
      <container-transaction>
         <method>
            <ejb-name>AuthorBean</ejb-name>
            <method-name>*</method-name>
         </method>
         <trans-attribute>Supports</trans-attribute>
      </container-transaction>
   </assembly-descriptor>
</ejb-jar>
